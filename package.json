{
  "name": "@verzth/tcx-js",
  "version": "1.1.0",
  "description": "TCX Client JS Library",
  "main": "tcx.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:js": "uglifyjs tcx.js -c -m -e -o tcx.min.js",
    "build:bundle": "uglifyjs node_modules/crypto-js/crypto-js.js node_modules/moment/min/moment-with-locales.js tcx.js -c -m -e -o tcx.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verzth/tcx-js.git"
  },
  "keywords": [
    "tcx",
    "tcx-client",
    "tcx-js"
  ],
  "author": "Dodi Triwibowo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/verzth/tcx-js/issues"
  },
  "homepage": "https://github.com/verzth/tcx-js#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "jquery": "^3.4.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.7.6"
  }
}
